{
  "baseStyles": {
    "flexItemsStyle": {},
    "flexContainerStyle": {
      "display": ["flex", {
        "moreInfo": "https://developer.mozilla.org/en-US/docs/Web/CSS/CSS_Flexible_Box_Layout/Using_CSS_flexible_boxes"
      }]
    },
    "directionExamples": [{
        "title": "row",
        "flexContainerStyle": {
          "flex-direction": ["row", {
            "isDefault": true,
            "moreInfo": "https://developer.mozilla.org/en-US/docs/Web/CSS/flex-direction"
          }]
        }
      },
      {
        "title": "column",
        "flexContainerStyle": {
          "flex-direction": ["column", {
            "moreInfo": "https://developer.mozilla.org/en-US/docs/Web/CSS/flex-direction"
          }]
        }
      }
    ]
  },
  "examples": [{
      "group": "Default values",
      "examplesInGroup": [{
        "title": "Everything default",
        "flexContainerStyle": {
          "justify-content": ["flex-start", {
            "isDefault": true
          }],
          "align-items": ["stretch", {
            "isDefault": true
          }],
          "align-content": ["stretch", {
            "isDefault": true
          }],
          "flex-wrap": ["nowrap", {
            "isDefault": true
          }]
        },
        "flexItemsStyle": {
          "order": ["0", {
            "isDefault": true
          }],
          "flex-basis": ["auto", {
            "isDefault": true
          }],
          "flex-shrink": ["1", {
            "isDefault": true
          }],
          "flex-grow": ["0", {
            "isDefault": true
          }],
          "align-self": ["auto", {
            "isDefault": true
          }]
        },
        "flexItemsStyleIdentical": {
          "order": ["0", {
            "isDefault": true
          }],
          "flex": ["0 1 auto", {
            "isDefault": true
          }]
        }
      }]
    },
    {
      "group": "Element Order",
      "examplesInGroup": [{
          "title": "Reverse the main axis order",
          "flexContainerStyle": {
            "flex-direction": ["-reverse"]
          }
        },
        {
          "title": "Change the order",
          "flexItemsStyle": {
            "order": ["{random}{0}{10}"]
          }
        }
      ]
    },
    {
      "group": "Element Wrapping",
      "examplesInGroup": [{
          "title": "No wrap",
          "flexContainerStyle": {
            "flex-wrap": ["nowrap", {
              "isDefault": true
            }]
          },
          "flexContainerStyleIdentical": {
            "flex-flow": ["nowrap dir"]
          }
        },
        {
          "title": "Normal wrap",
          "flexContainerStyle": {
            "flex-wrap": ["wrap"]
          },
          "flexContainerStyleIdentical": {
            "flex-flow": ["wrap dir"]
          }
        },
        {
          "title": "Reverse wrap, lines from bottom up",
          "flexContainerStyle": {
            "flex-wrap": ["wrap-reverse"]
          },
          "flexContainerStyleIdentical": {
            "flex-flow": ["wrap-reverse dir"]
          }
        }
      ]
    },
    {
      "group": "Distribute items along main axis",
      "examplesInGroup": [{
          "title": "Stacked from start",
          "flexContainerStyle": {
            "justify-content": ["flex-start", {
              "isDefault": true
            }]
          }
        },
        {
          "title": "Stacked from end",
          "flexContainerStyle": {
            "justify-content": ["flex-end"]
          }
        },
        {
          "title": "Centered",
          "flexContainerStyle": {
            "justify-content": ["center"]
          }
        },
        {
          "title": "Equal space between items",
          "flexContainerStyle": {
            "justify-content": ["space-between"]
          }
        },
        {
          "title": "Equal space around items",
          "flexContainerStyle": {
            "justify-content": ["space-around", {
              "moreInfo": "https://developer.mozilla.org/en-US/docs/Web/CSS/justify-content"
            }]
          }
        }
      ]
    },
    {
      "group": "Distribute items along cross axis' lines",
      "examplesInGroup": [{
          "title": "Fill avaible space of cross axis",
          "flexContainerStyle": {
            "flex-wrap": ["wrap"],
            "align-items": ["stretch", {
              "isDefault": true
            }]
          }
        },
        {
          "title": "Start of cross axis",
          "flexContainerStyle": {
            "flex-wrap": ["wrap"],
            "align-items": ["flex-start"]
          }
        },
        {
          "title": "Items baseline",
          "flexContainerStyle": {
            "flex-wrap": ["wrap"],
            "align-items": ["baseline"]
          }
        },
        {
          "title": "End of cross axis",
          "flexContainerStyle": {
            "flex-wrap": ["wrap"],
            "align-items": ["flex-end"]
          }
        },
        {
          "title": "Center of cross axis",
          "flexContainerStyle": {
            "flex-wrap": ["wrap"],
            "align-items": ["center"]
          }
        }
      ]
    },
    {
      "group": "Distribute lines along cross axis",
      "examplesInGroup": [{
          "title": "Fill avaible space of cross axis",
          "flexContainerStyle": {
            "flex-wrap": ["wrap"],
            "align-content": ["stretch", {
              "isDefault": true
            }]
          }
        },
        {
          "title": "Start of cross axis",
          "flexContainerStyle": {
            "flex-wrap": ["wrap"],
            "align-content": ["flex-start"]
          }
        },
        {
          "title": "End of cross axis",
          "flexContainerStyle": {
            "flex-wrap": ["wrap"],
            "align-content": ["flex-end"]
          }
        },
        {
          "title": "Center of cross axis",
          "flexContainerStyle": {
            "flex-wrap": ["wrap"],
            "align-content": ["center"]
          }
        },
        {
          "title": "Space between",
          "flexContainerStyle": {
            "flex-wrap": ["wrap"],
            "align-content": ["space-between"]
          }
        },
        {
          "title": "Space around",
          "flexContainerStyle": {
            "flex-wrap": ["wrap"],
            "align-content": ["space-around"]
          }
        }
      ]
    },
    {
      "group": "Grow and shrink",
      "examplesInGroup": [{
          "title": "Grow with default shrink",
          "flexItemsStyle": {
            "flex-grow": ["1"],
            "flex-basis": ["auto", {
              "isDefault": true
            }],
            "flex-shrink": ["1", {
              "isDefault": true
            }]
          },
          "flexItemsStyleIdentical": {
            "flex": ["1 1 auto"]
          }
        },
        {
          "title": "Grow but no shrink",
          "flexItemsStyle": {
            "flex-grow": ["1"],
            "flex-basis": ["auto", {
              "isDefault": true
            }],
            "flex-shrink": ["0"]
          },
          "flexItemsStyleIdentical": {
            "flex": ["1 0 auto"]
          }
        },
        {
          "title": "Shrink but no grow",
          "flexItemsStyle": {
            "flex-shrink": ["1", {
              "isDefault": true
            }],
            "flex-grow": ["0", {
              "isDefault": true
            }],
            "flex-basis": ["auto", {
              "isDefault": true
            }]
          },
          "flexItemsStyleIdentical": {
            "flex": ["0 1 auto", {
              "isDefault": true
            }]
          }
        },
        {
          "title": "Grow relative to each other",
          "flexItemsStyle": {
            "flex-grow": ["{random}{0}{5}"],
            "flex-basis": ["0", {
              "isDefault": true
            }],
            "flex-shrink": ["0"]
          },
          "flexItemsStyleIdentical": {
            "flex": ["X 0 0"]
          }
        },
        {
          "title": "Shrink relative to each other",
          "flexItemsStyle": {
            "flex-shrink": ["{random}{1}{5}"],
            "flex-basis": ["0", {
              "isDefault": true
            }],
            "flex-grow": ["0", {
              "isDefault": true
            }]
          },
          "flexItemsStyleIdentical": {
            "flex": ["0 X auto"]
          }
        },
        {
          "title": "Basis overrides with",
          "flexItemsStyle": {
            "flex-basis": ["50%"],
            "flex-shrink": ["0"],
            "flex-grow": ["0", {
              "isDefault": true
            }]
          },
          "flexItemsStyleIdentical": {
            "flex": ["0 0 50%"]
          }
        },
        {
          "title": "All items grows equal from there basis",
          "flexItemsStyle": {
            "flex-basis": ["{random}{0}{3}[\"100px\", \"200px\", \"300px\"]"],
            "flex-shrink": ["1"],
            "flex-grow": ["1"]
          },
          "flexItemsStyleIdentical": {
            "flex": ["1 1 X"]
          }
        }
      ]
    },
    {
      "group": "Distribute items separetly",
      "examplesInGroup": [{
          "title": "Align self",
          "flexItemsStyle": {
            "align-self": ["{random}{0}{10}[\"center\", \"flex-end\", \"flex-start\", \"stretch\"]"]
          },
          "flexContainerStyle": {
            "justify-content": ["center"],
            "align-items": ["stretch", {
              "isDefault": true
            }],
            "flex-wrap": ["wrap"]
          }
        }
      ]
    }
  ]
}